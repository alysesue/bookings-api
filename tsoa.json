{
	"swagger": {
      "outputDirectory": "./dist/swagger",
		"entryFile": "./src/index.ts",
		"specVersion": 3,
		"yaml": true,
      "host": "localhost:3999",
      "schemes": [
        "http",
        "https"
      ]
	},
	"routes": {
		"entryFile": "./src/controllers.ts",
		"routesDir": "./src",
		"middleware": "koa",
		"middlewareTemplate": "./configs/shared-config/tsoa-koa.hbs"
	}
}
